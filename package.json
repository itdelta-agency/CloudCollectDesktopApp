{
  "name": "cloudcollect",
  "version": "0.0.3",
  "description": "",
  "main": "main.js",
  "build": {
    "appId": "com.cloudcollect.desktop",
    "productName": "CloudCollect",
    "win": {
      "target": [
        "appx"
      ],
      "icon": "assets/icon.ico"
    },
    "appx": {
      "publisher": "CN=27A3596C-0577-4E38-B86F-EB1D91BCC0F6",
      "publisherDisplayName": "Stohn Advokatanpartsselskab",
      "identityName": "StohnAdvokatanpartsselska.CloudCollect",
      "backgroundColor": "#ffffff",
      "startupTasks": [
        {
          "taskId": "CloudCollectStartup",
          "enabled": true
        }
    ]
    }
  },
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "dist:store": "electron-builder --win msix",
    "pack": "electron-packager . CloudCollect --platform=win32 --arch=x64 --out=dist --overwrite",
    "msix": "yes | electron-windows-store --input-directory dist/CloudCollect-win32-x64 --output-directory store-app --package-version 0.0.1 --package-name CloudCollect --identity-name StohnAdvokatanpartsselska.CloudCollect --publisher CN=27A3596C-0577-4E38-B86F-EB1D91BCC0F6 --publisher-display-name \"Stohn Advokatanpartsselskab\""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^33.2.0",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2",
    "electron-windows-store": "^2.1.0"
  },
  "dependencies": {
    "electron-log": "^5.3.1"
  }
}
