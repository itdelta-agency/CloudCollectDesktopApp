{
  "name": "cloudcollectapp",
  "version": "1.1.39",
  "description": "",
  "main": "main.js",
  "build": {
    "appId": "com.cloudcollect.desktop",
    "productName": "CloudCollectApp",
    "win": {
      "target": [
        "appx"
      ],
      "icon": "assets/icon.ico"
    },
    "appx": {
      "publisher": "CN=YourPublisherFromPartnerCenter",
      "publisherDisplayName": "Your Company Name",
      "identityName": "CloudCollectApp",
      "backgroundColor": "#ffffff"
    }
  },
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "dist:store": "electron-builder --win msix",
    "pack": "electron-packager . CloudCollectApp --platform=win32 --arch=x64 --out=dist --overwrite",
    "msix": "yes | electron-windows-store --input-directory dist/CloudCollectApp-win32-x64 --output-directory store-app --package-version 1.0.0.0 --package-name CloudCollectApp --identity-name CloudCollectApp --publisher CN=Your-Publisher --publisher-display-name \"Your Company\""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^33.2.0",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2",
    "electron-windows-store": "^2.1.0"
  },
  "dependencies": {
    "electron-log": "^5.3.1",
    "electron-store": "^8.2.0"
  }
}
