{
  "name": "cloudcollectapp",
  "version": "1.1.39",
  "description": "",
  "main": "main.js",
  "build": {
    "appId": "com.cloudcollect.desktop",
    "productName": "CloudCollectApp",
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico",
      "artifactName": "CloudCollectApp-Setup-${version}.${ext}"
    },
    "msix": {
      "displayName": "CloudCollectApp",
      "identityName": "com.cloudcollect.desktop",
      "packageName": "CloudCollectApp",
      "publisherDisplayName": "Your Company Name",
      "publisher": "CN=YOUR-COMPANY-NAME",
      "backgroundColor": "#ffffff",
      "startMenu": true,
      "startOnUserLogin": true,
      "store": true
    }
  },
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "dist:store": "electron-builder --win msix",
    "pack": "electron-packager . CloudCollectApp --platform=win32 --arch=x64 --out=dist --overwrite",
    "msix": "echo Y | electron-windows-store --input-directory dist/CloudCollectApp-win32-x64 --output-directory store-app --package-version 1.0.0.0 --package-name CloudCollectApp --identity-name CloudCollectApp --publisher CN=Your-Publisher --publisher-display-name \"Your Company\""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^33.2.0",
    "electron-packager": "^17.1.2",
    "electron-windows-store": "^2.1.0"
  },
  "dependencies": {
    "electron-log": "^5.3.1"
  }
}
